{
  "name": "asdf",
  "id": "asdf",
  "version": "0.1.7",
  "description": "Install asdf from github releases.",
  "options": {
    "favorite": {
      "type": "string",
      "default": "0.18.0",
      "description": "The version of asdf to install."
    }
  },
  "onCreateCommand": {
    "pnpm-store-persistence": "/usr/local/share/asdf/scripts/oncreate.sh"
  },
  "updateContentCommand": {
    "pnpm-store-persistence": "/usr/local/share/asdf/scripts/updatecontent.sh"
  },
  "dependsOn": [
    "ghcr.io/devcontainers/features/common-utils"
  ]
}
